{
  "name": "diet-pc-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint:all": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-staged": "lint-staged",
    "lint-staged:all": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier-eslint --write 'src/**/*'",
    "release": "standard-version -a && git push --follow-tags"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/classnames": "^2.3.1",
    "@types/crypto-js": "^4.0.2",
    "@types/echarts": "^4.9.9",
    "@types/lodash-es": "^4.17.4",
    "@types/nprogress": "^0.2.0",
    "@types/numeral": "^2.0.1",
    "@types/qs": "^6.9.7",
    "@types/react-router-dom": "^5.1.8",
    "antd": "^4.16.10",
    "array-move": "^3.0.1",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.6",
    "lodash-es": "^4.17.21",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.4",
    "swr": "^0.5.6",
    "teaness": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@svgr/rollup": "^5.5.0",
    "@types/ejs": "^3.0.7",
    "@types/html-minifier-terser": "^5.1.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/svgr__rollup": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@typescript-eslint/typescript-estree": "^4.29.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "global-agent": "^3.0.0",
    "jest": "^27.0.6",
    "less": "^4.1.1",
    "lint-staged": "^11.1.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "sass": "^1.37.5",
    "standard-version": "^9.3.1",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vite-plugin-html": "^2.0.7",
    "yorkie": "^2.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier-eslint --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint:all"
  },
  "gitHooks": {
    "pre-commit": "tsc --noEmit && lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "optionalDependencies": {
    "commander": "^7.2.0",
    "ts-node": "^9.1.1"
  }
}
